{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ],
  "functions": {
    "index.ts": {
      "maxDuration": 30
    }
  },
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  }
}